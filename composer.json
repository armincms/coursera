{
    "name": "armincms/coursera",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0", 
        "armincms/categorizable": "^2.2",
        "phoenix-lib/nova-nested-tree-attach-many": "^1.8",
        "whitecube/nova-flexible-content": "~0.2.10",
        "armin-widget/slider": "*"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Coursera\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Coursera\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
